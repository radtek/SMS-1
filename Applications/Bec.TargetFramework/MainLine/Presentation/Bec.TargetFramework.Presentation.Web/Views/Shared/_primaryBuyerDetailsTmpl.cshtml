<dl class="dl-horizontal">
    <dt>Full Name:</dt>
    <dd><p>{{fullName}}</p></dd>
    <dt>Email:</dt>
    <dd><p>{{UserAccountOrganisation.UserAccount.Email}}</p></dd>
    {{#if formattedBirthDate}}
    <dt>Date of Birth:</dt>
    <dd><p>{{formattedBirthDate}}</p></dd>
    {{/if}}
    {{#if UserAccountOrganisation.UserAccount.IsTemporaryAccount}}
    <dt>PIN:</dt>
    <dd><p>{{UserAccountOrganisation.PinCode}}</p></dd>
    {{/if}}
    {{#if formattedLastLogin}}
    <dt>Last Logged In:</dt>
    <dd><p>{{formattedLastLogin}}</p></dd>
    {{/if}}
    {{#if formattedLatestBankAccountCheckOn}}
    <dt>Bank Account Check:</dt>
    <dd><p>{{formattedLatestBankAccountCheckOn}}</p></dd>
    {{/if}}
</dl>
