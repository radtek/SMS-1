@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<div id="content">
    <div class="row">
        <article class="col-sm-12">
            <div class="alert alert-danger fade in">
                <i class="fa-fw fa fa-times"></i>
                <strong>Error!</strong> @Model.Exception.Message
            </div>
        </article>
    </div>
</div>
