<div class="row" id="bankAccountRow{{index}}">
    <section class="col col-6">
        <label class="label">Account Number</label>
        <label class="input">
            <input type="text" name="SmsSrcFundsBankAccounts[{{index}}].AccountNumber" maxlength="8" data-rule-required="true" data-rule-digits="true" data-rule-minlength="8" />
        </label>
    </section>
    <section class="col col-5">
        <label class="label">Sort Code</label>
        <label class="input">
            <input type="text" name="SmsSrcFundsBankAccounts[{{index}}].SortCode" maxlength="6" data-rule-required="true" data-rule-digits="true" data-rule-minlength="6" />
        </label>
    </section>
    <section class="col-sm-1">
        <label class="label">&nbsp;</label>
        <a data-parent-id="bankAccountRow{{index}}" class="delete-entry"><i class="fa fa-minus margin-right-10"></i></a>
    </section>
</div>