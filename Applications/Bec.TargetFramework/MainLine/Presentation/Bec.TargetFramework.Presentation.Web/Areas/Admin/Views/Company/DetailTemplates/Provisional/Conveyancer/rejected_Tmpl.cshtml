<div class="col-sm-6">
    <!-- well -->
    <div class="well">
        <h5>Company</h5>
        <dl class="dl-horizontal">
            <dt>Name:</dt>
            <dd><p>{{Name}}</p></dd>
            <dt>Address:</dt>
            <dd><p>{{Line1}}</p></dd>
            <dd><p>{{Line2}}</p></dd>
            <dd><p>{{Town}}</p></dd>
            <dd><p>{{County}}</p></dd>
            <dd><p>{{PostalCode}}</p></dd>
            <dt>Regulator:</dt>
            <dd><p>{{RegulatorNameDisplay}}</p></dd>
            <dt>Regulator Number:</dt>
            <dd><p>{{RegulatorNumber}}</p></dd>
        </dl>
    </div>
</div>
<div class="col-sm-6">
    <!-- well -->
    <div class="well">
        <h5>Organisation Administrator</h5>
        <dl class="dl-horizontal">
            <dt>Name:</dt>
            <dd><p>{{SystemAdminNameDisplay}}</p></dd>
            <dt>Email:</dt>
            <dd><p>{{OrganisationAdminEmail}}</p></dd>
        </dl>
        <h5>Other Details</h5>
        <dl class="dl-horizontal">
            <dt>Rejected On:</dt>
            <dd><p>{{StatusChangedOnDisplay}}</p></dd>
            <dt>Rejected By:</dt>
            <dd><p>{{StatusChangedBy}}</p></dd>
            <dt>Rejected Reason:</dt>
            <dd><p>{{Reason}}</p></dd>
            <dt>Rejected Notes:</dt>
            <dd><p>{{Notes}}</p></dd>
            <dt>Referrer Source:</dt>
            <dd><p>{{Referrer}}</p></dd>
            <dt>Scheme ID:</dt>
            <dd><p>{{SchemeID}}</p></dd>
            <dt>Files per Month:</dt>
            <dd><p>{{FilesPerMonth}}</p></dd>
        </dl>
    </div>
</div>