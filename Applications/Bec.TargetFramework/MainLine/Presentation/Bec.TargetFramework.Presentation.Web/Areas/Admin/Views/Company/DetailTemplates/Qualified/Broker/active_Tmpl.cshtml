<div class="col-sm-6">
    <!-- well -->
    <div class="well">
        <h5>Company</h5>
        <dl class="dl-horizontal">
            <dt>Name:</dt>
            <dd><p>{{Name}}</p></dd>
            <dt>Address:</dt>
            <dd><p>{{Line1}}</p></dd>
            <dd><p>{{Line2}}</p></dd>
            <dd><p>{{Town}}</p></dd>
            <dd><p>{{County}}</p></dd>
            <dd><p>{{PostalCode}}</p></dd>
            <dt>Broker Type:</dt>
            <dd><p>{{BrokerTypeDescription}}</p></dd>
            <dt>Broker Business Type:</dt>
            <dd><p>{{BrokerBusinessTypeDescription}}</p></dd>
        </dl>
    </div>
</div>
<div class="col-sm-6">
    <!-- well -->
    <div class="well">
        <h5>Organisation Administrator</h5>
        <dl class="dl-horizontal">
            <dt>Name:</dt>
            <dd><p>{{SystemAdminNameDisplay}}</p></dd>
            <dt>Email:</dt>
            <dd><p>{{OrganisationAdminEmail}}</p></dd>
        </dl>
        <h5>Verified Details</h5>
        <dl class="dl-horizontal">
            <dt>Verified On:</dt>
            <dd><p>{{VerifiedOnDisplay}}</p></dd>
            <dt>Verified By:</dt>
            <dd><p>{{VerifiedBy}}</p></dd>
        </dl>
        <h5>Other Details</h5>
        <dl class="dl-horizontal">
            <dt>Created On:</dt>
            <dd><p>{{CreatedOnDisplay}}</p></dd>
            <dt>Referrer Source:</dt>
            <dd><p>{{Referrer}}</p></dd>
        </dl>
    </div>
</div>