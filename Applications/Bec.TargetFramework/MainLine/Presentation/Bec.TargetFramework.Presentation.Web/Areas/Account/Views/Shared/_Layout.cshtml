<!DOCTYPE html>
<html lang="en-us" id="@ViewBag.PageId" class="@Settings.CurrentTheme">
<head>
    <title>Submit Details - Safe Move Scheme</title>
    <meta name="description" content="">
    <meta name="keywords" content="Safe Move Scheme, property transactions, manage risk, Prevent property fraud, property fraud, buyers, Reduce risk, lenders, conveyancers, Safe Seller, Safe buyer">

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- iOS web-app metas : hides Safari UI Components and Changes Status Bar Appearance -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">

    <!-- FAVICONS -->
    <link rel="shortcut icon" href="~/content/img/favicon/favicon.ico" type="image/x-icon">
    <link rel="icon" href="~/content/img/favicon/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="~/content/img/splash/sptouch-icon-iphone.png">
    <link rel="apple-touch-icon" sizes="76x76" href="~/content/img/splash/touch-icon-ipad.png">
    <link rel="apple-touch-icon" sizes="120x120" href="~/content/img/splash/touch-icon-iphone-retina.png">
    <link rel="apple-touch-icon" sizes="152x152" href="~/content/img/splash/touch-icon-ipad-retina.png">

    <!-- Startup image for web apps -->
    <link rel="apple-touch-startup-image" href="~/content/img/splash/ipad-landscape.png" media="screen and (min-device-width: 481px) and (max-device-width: 1024px) and (orientation:landscape)">
    <link rel="apple-touch-startup-image" href="~/content/img/splash/ipad-portrait.png" media="screen and (min-device-width: 481px) and (max-device-width: 1024px) and (orientation:portrait)">
    <link rel="apple-touch-startup-image" href="~/content/img/splash/iphone.png" media="screen and (max-device-width: 320px)">

    @Styles.Render("~/content/websitelayout/css/style")

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script src='https://www.google.com/recaptcha/api.js'></script>

    @RenderSection("Scripts", false)
</head>
<body class="@ViewBag.PageClass @Settings.CurrentTheme @Html.RouteIf("hidden-menu","AcceptTCs", "Index")">

    <div class="container">
        <div class="top navbar square-bottom-right">
            <a href="http://www.safemovescheme.co.uk/">
                <img class="brand-img" src="~/content/websitelayout/images/SMS-logo.png" />
            </a>
        </div>
    </div>
    <div class="container">
        <nav class="navbar navbar-default square-top-right">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand heading-toggle" href="http://www.safemovescheme.co.uk/">Home</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle @Html.Conditional("selected", "action", "ThreatsExplained", "BuyersAtRisk", "ConveyancersAtRisk")" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Fraud Threat <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li class="@Html.Conditional("selected", "action", "ThreatsExplained")"><a href="http://www.safemovescheme.co.uk/Home/ThreatsExplained">Threats Explained</a></li>
                            <li class="@Html.Conditional("selected", "action", "BuyersAtRisk")"><a href="http://www.safemovescheme.co.uk/Home/BuyersAtRisk">Buyers at Risk</a></li>
                            <li class="@Html.Conditional("selected", "action", "ConveyancersAtRisk")"><a href="http://www.safemovescheme.co.uk/Home/ConveyancersAtRisk">Conveyancers At Risk</a></li>
                        </ul>
                    </li>

                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle @Html.Conditional("selected", "action", "HowItWorks", "SafeBuyerSpecification", "SafeMoveSchemeBenefits")" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Safe Buyer Product <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li class="@Html.Conditional("selected", "action", "HowItWorks")"><a href="http://www.safemovescheme.co.uk/Home/HowItWorks">How It Works</a></li>
                            <li class="@Html.Conditional("selected", "action", "SafeBuyerSpecification")"><a href="http://www.safemovescheme.co.uk/Home/SafeBuyerSpecification">Safe Buyer Specification</a></li>
                            <li class="@Html.Conditional("selected", "action", "SafeMoveSchemeBenefits")"><a href="http://www.safemovescheme.co.uk/Home/SafeMoveSchemeBenefits">Safe Move Scheme Benefits</a></li>
                        </ul>
                    </li>

                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle @Html.Conditional("selected", "action", "FreeTrial", "NewAMLSolution", "ConveyancersCompliance", "LendersCompliance", "ConveyancingFirmRegistration")" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">For Professionals <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li class="@Html.Conditional("selected", "action", "FreeTrial")"><a href="http://www.safemovescheme.co.uk/Home/FreeTrial">** Free Safe Buyer Trial **</a></li>
                            <li class="@Html.Conditional("selected", "action", "NewAMLSolution")"><a href="http://www.safemovescheme.co.uk/Home/NewAMLSolution">New AML Solution</a></li>
                            <li class="@Html.Conditional("selected", "action", "ConveyancersCompliance")"><a href="http://www.safemovescheme.co.uk/Home/ConveyancersCompliance">Conveyancer's Compliance</a></li>
                            <li class="@Html.Conditional("selected", "action", "LendersCompliance")"><a href="http://www.safemovescheme.co.uk/Home/LendersCompliance">Lender's Compliance</a></li>
                            <li class="@Html.Conditional("selected", "action", "ConveyancingFirmRegistration")"><a href="http://www.safemovescheme.co.uk/Home/ConveyancingFirmRegistration">Conveyancing Firm Registration</a></li>
                        </ul>
                    </li>
                    <li class="@Html.Conditional("selected", "action", "ForThePublic")"><a href="http://www.safemovescheme.co.uk/Home/ForThePublic">For the Public</a></li>
                    <li class="@Html.Conditional("selected", "action", "AboutUs")"><a href="http://www.safemovescheme.co.uk/Home/AboutUs">About us</a></li>

                    <li class="@Html.Conditional("selected", "controller", "Login") nav-login"><a href="@Url.Action("Index", "Login", new { Area = "Account" })">Login&nbsp;<i class="fa fa-sign-in"></i></a></li>
                </ul>
            </div>
        </nav>
    </div>
    <div class="container body-content">
        @RenderBody()
    </div>
    <footer class="footer">
        <div class="container">
            <div class="header-social">
                <a target="_blank" href="https://www.facebook.com"><i class="fa fa-facebook"></i></a>
                <a target="_blank" href="https://www.twitter.com"><i class="fa fa-twitter"></i></a>
                <a target="_blank" href="https://www.youtube.com"><i class="fa fa-youtube"></i></a>
                <a target="_blank" href="https://www.linkedin.com"><i class="fa fa-linkedin"></i></a>
            </div>
            <p>&copy; @DateTime.Now.Year Safe Move Scheme. All Rights Reserved.<br /></p>
            <p><a href="http://www.safemovescheme.co.uk/PDF/SMS-T%26C's.pdf" target="_blank">Terms of Use & Privacy Policy</a>&nbsp;|&nbsp;<a href="http://www.safemovescheme.co.uk/Home/ContactUs">Contact us</a></p>

        </div>
    </footer>
        @Scripts.Render("~/scripts/websitelayout")

    </body>
</html>
